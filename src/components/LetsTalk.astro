<style>
    article{
        color: white;
        text-align: center;
        margin-bottom: 5em;

        & img{
            width: 85%;
            padding: 15px;
        }

        & button{
            background-color: transparent;
            padding: 15px;
            border-radius: 25px;
        }

        & button:hover{
            background-color: rgb(69, 69, 69);
        }
    }

    a{
        text-decoration: none;
        color: white;
        font-size: larger;
    }

    .SoMe{
        display: flex;
        justify-content: center;
        gap: 20px;
    }
    .SoMe a:hover{
        background-color: rgba(255, 255, 255, 0.262);
    }

    .SoMe a{
        border-style: solid;
        border-width: thin;
        border-color: white;
        padding: 20px;
        font-size: small;
    }

    p{
        width: 80%;
        margin: auto;
    }
    @media screen and (min-width: 599px){
    .SoMe a{
        border-style: solid;
        border-width: thin;
        border-color: white;
        padding: 20px;
    }

    .SoMe{
        margin-bottom: 2em;
    }

    button{
        padding: 10px 20px;
        background: transparent;
        margin-top: 2em;
        margin-bottom: 2em;
        border-radius: 25px;

        & a{
            font-size: x-large;
        }
    }

}


@media screen and (min-width: 999px){
    #LetsTalk img{
        width: 50%;
    }
    
}
</style>
















